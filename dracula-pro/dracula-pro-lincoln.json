  "tokenColors": [
    {
      "scope": [
        "emphasis"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "strong"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "header"
      ],
      "settings": {
        "foreground": "#9580FF"
      }
